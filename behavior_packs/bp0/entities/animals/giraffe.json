{
    "format_version": "1.13.0",
    "minecraft:entity": {

      "description": {
        "identifier": "animal:giraffe",
        "is_spawnable": true,
        "is_summonable": true,
        "is_experimental": false
      },


      "component_groups": {

        
        "cg:interact" : {
          "minecraft:interact": {
            "interactions": [
              {
                "on_interact": {  "filters": {"all_of": [  {"test": "has_equipment","domain": "hand","subject": "other","value": "gameplay:cotton_bud"} ]}},
                "use_item": true,
                "swing": true,
                "interact_text" : "action.interact.take_dna",
                "spawn_items": {"table": "loot_tables/dna/tuto.json"},
                "play_sounds": "random.pop"
               }
            ]
          }
        },

        "minecraft:baby": {
            "minecraft:is_baby": {
            },
            "minecraft:scale": {
              "value": 0.8
            },
            "minecraft:ageable": {
              "duration": 1200,
              "feedItems": [           
                "wheat" ],
              "grow_up": {
                "event": "minecraft:ageable_grow_up",
                "target": "self"
              }
            },
            "minecraft:behavior.follow_parent": {
              "priority": 6,
              "speed_multiplier": 1.1
            }
        },


        "minecraft:adult": {
          "minecraft:loot": {
            "table": "loot_tables/entities/giraffe.json"
          },
            "minecraft:experience_reward": {
              "on_bred": "Math.Random(1,7)",
              "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
            },
            "minecraft:scale": {     "value": 1.4   },
            "minecraft:behavior.breed": {
              "priority": 4,
              "speed_multiplier": 1.0
            },
            "minecraft:breedable": {
              "requireTame": false,
              "breedsWith": {
                "mateType": "animal:giraffe",
                "babyType": "animal:giraffe",
                "breed_event": {
                  "event": "minecraft:entity_born",
                  "target": "baby"
                }
              },
              "breedItems": [           
                "wheat" ]
            },
            "minecraft:tameable": {
              "probability": 0.20,
              "tame_items": [
                "apple"
              ],
              "tame_event": {
                "event": "minecraft:on_tame",
                "target": "self"
              }
            }
        },

        "minecraft:tamed":{
          "minecraft:is_tamed": {},
          "minecraft:rideable": {
            "seat_count": 1,
            "interact_text": "action.interact.mount",
            "family_types": [
              "player"
            ],
            "seats": {
              "position": [0, 1.7, -0.1]
            }
          }
        },

        "minecraft:un_saddled":{
          "minecraft:interact": {
            "interactions": [
              {
                "play_sounds": "armor.equip_generic",
                "on_interact": {
                  "filters": {
                    "all_of": [
                      { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "saddle"},
                      { "test" :  "is_family", "subject" : "other", "value" :  "player"}
                    ]
                  },
                  "event": "minecraft:on_saddle",
                  "target": "self"
                },
                "use_item": true,
                "interact_text": "action.interact.attachSadle"
              }
            ]
          }
        },


        "minecraft:saddled":{
          "minecraft:input_ground_controlled": {},
          "minecraft:behavior.player_ride_tamed": {},
          "minecraft:is_saddled":{}
        }

      },
  
  
      "components": {

        "minecraft:type_family": {     "family": [ "mob" , "giraffe" ]     },
        "minecraft:physics": {},
        "minecraft:health": {    "value": 20,   "max": 20   },
        "minecraft:movement": {  "value": 0.26 },
        "minecraft:navigation.walk": {   "can_path_over_water": true,   "avoid_water": true,   "avoid_damage_blocks": true   },
        "minecraft:movement.basic": {},
        "minecraft:jump.static": {},
        "minecraft:can_climb": {},
        "minecraft:collision_box": {
            "width": 0.9,
            "height": 2
        },
        "minecraft:behavior.mount_pathing": {
          "priority": 1,
          "speed_multiplier": 1.25,
          "target_dist": 0.0,
          "track_target": true
        },
        "minecraft:behavior.float": {
          "priority": 2
        },
        "minecraft:behavior.panic": {
          "priority": 3,
          "speed_multiplier": 1.25
        },
        "minecraft:behavior.random_stroll": {
          "priority": 7,
          "speed_multiplier": 1.0
        },
        "minecraft:behavior.look_at_player": {
          "priority": 8,
          "look_distance": 6.0,
          "probability": 0.02
        },
        "minecraft:behavior.random_look_around": {
          "priority": 9
        },
        "minecraft:behavior.run_around_like_crazy": {
          "priority": 1,
          "speed_multiplier": 1.2
        },
        "minecraft:behavior.nap": {
          "priority": 8,
          "cooldown_min": 20.0,
          "cooldown_max": 30.0,
          "mob_detect_dist": 6.0,
          "mob_detect_height": 6.0,
          "can_nap_filters": {
            "all_of": [
              {
                "test": "in_water",
                "subject": "self",
                "operator": "==",
                "value": false
              },
              {
                "test": "on_ground",
                "subject": "self",
                "operator": "==",
                "value": true
              }
            ]
          },
          "wake_mob_exceptions": {
            "any_of": [
              {
                "test": "is_family",
                "subject": "other",
                "operator": "==",
                "value": "camel"
              }
            ]
          }
        },
        "minecraft:behavior.stay_while_sitting": {
          "priority": 3
        }
      },
  
  
      "events": {
        "minecraft:entity_spawned": {
          "randomize": [
            {
              "weight": 9,
              "remove": {},
              "add": { "component_groups": ["minecraft:adult", "cg:interact"]}
            },
            {
              "weight": 3,
              "remove": {},
              "add": {"component_groups": ["minecraft:baby", "cg:interact"]}
            }
          ]
        },
        "minecraft:entity_born": {
          "add": {  "component_groups": [ "minecraft:baby" , "cg:interact"]  }
        },
        "minecraft:ageable_grow_up": {
            "remove": {   "component_groups": ["minecraft:baby"]  },
            "add": {  "component_groups": [   "minecraft:adult"    ]}
        },
        "minecraft:on_tame":{
          "add": {  "component_groups": [   "minecraft:tamed" , "minecraft:un_saddled"   ]}
        },

        "minecraft:on_saddle":{
          "remove": {   "component_groups": ["minecraft:un_saddled"]  },
          "add": {  "component_groups": [   "minecraft:saddled"    ]}
        }

      }


    }
  }
  