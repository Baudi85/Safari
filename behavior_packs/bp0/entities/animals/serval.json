{
  "format_version": "1.8.0",
  "minecraft:entity": {

    "description": {
      "identifier": "animal:serval",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },


    "component_groups": {

      "cg:interact" : {
        "minecraft:interact": [
          {
           "on_interact": {  "filters": {"all_of": [  {"test": "has_equipment","domain": "hand","subject": "other","value": "gameplay:cotton_bud"} ]}},
           "use_item": true,
           "swing": true,
           "interact_text" : "action.interact.take_dna",
           "spawn_items": {"table": "loot_tables/dna/20.json"},
           "play_sounds": "random.pop"
          }
        ]
      },
      "minecraft:baby": {
          "minecraft:is_baby": {
          },
          "minecraft:scale": {
            "value": 0.7
          },
          "minecraft:ageable": {
            "duration": 1200,
            "feed_items": [ "fish", "salmon", "chicken" ],
            "grow_up": {
              "event": "minecraft:ageable_grow_up",
              "target": "self"
            }
          },
          "minecraft:behavior.follow_parent": {
            "priority": 6,
            "speed_multiplier": 1.1
          }
      },


      "minecraft:adult": {
        "minecraft:loot": {
          "table": "loot_tables/entities/serval.json"
        },
          "minecraft:experience_reward": {
            "on_bred": "Math.Random(1,7)",
            "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
          },
          "minecraft:scale": {     "value": 1   },
          "minecraft:behavior.breed": {
            "priority": 4,
            "speed_multiplier": 1.0
          }
        },


        "minecraft:wild":{
          "minecraft:trusting": {
            "probability": 0.33,
            "trust_items": [ "chicken" ],
            "trust_event": {
              "event": "minecraft:on_trust",
              "target": "self"
            }
          },
          "minecraft:behavior.tempt": {
            "priority": 4,
            "speed_multiplier": 0.5,
            "within_radius": 16,
            "can_get_scared": true,
            "items": [  "chicken"
            ]
          },
          "minecraft:behavior.avoid_mob_type": {
            "priority": 5,
            "entity_types": [
              {
                "filters": {
                  "test": "is_family",
                  "subject": "other",
                  "value": "player"
                },
                "max_dist": 10,
                "walk_speed_multiplier": 0.8,
                "sprint_speed_multiplier": 1.33
              }
            ]
          }
        },


        "minecraft:trusting": {
          "minecraft:behavior.breed": {
            "priority": 3,
            "speed_multiplier": 1.0
          },
          "minecraft:behavior.tempt": {
            "priority": 4,
            "speed_multiplier": 0.5,
            "within_radius": 16,
            "items": [ "chicken"
            ]
          },
          "minecraft:breedable": {
            "require_tame": false,
            "breeds_with": {
              "mate_type": "animal:serval",
              "baby_type": "animal:serval",
              "breed_event": {
                "event": "minecraft:entity_born",
                "target": "baby"
              }
            },
            "breed_items": [ "fish", "salmon"]
          }
        }
    },


    "components": {

      "minecraft:type_family": {     "family": [ "mob" , "ocelot", "serval"]     },
      "minecraft:physics": {},
      "minecraft:health": {    "value": 10,   "max": 10   },
      "minecraft:movement": {  "value": 0.3 },
      "minecraft:navigation.walk": {   "can_path_over_water": true,   "avoid_water": true,   "avoid_damage_blocks": true   },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:nameable": {},
      "minecraft:collision_box": {
          "width": 0.8,
          "height": 1
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 1,
        "speed_multiplier": 1.25,
        "target_dist": 0.0,
        "track_target": true
      },
      "minecraft:behavior.float": {
        "priority": 2
      },
      "minecraft:behavior.panic": {
        "priority": 3,
        "speed_multiplier": 1.25
      },
      "minecraft:behavior.random_stroll": {
        "priority": 7,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 8,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 9
      },
      "minecraft:attack_damage": {
        "value": 4
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 1,
        "attack_interval": 10,
        "reselect_targets": true,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "chicken"
            },
            "max_dist": 8
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "baby_turtle"
                },
                {
                  "test": "in_water",
                  "subject": "other",
                  "operator": "!=",
                  "value": true
                }
              ]
            },
            "max_dist": 8
          }
        ]
      },
      "minecraft:behavior.leap_at_target": {
        "priority": 3,
        "target_dist": 0.3
      },
      "minecraft:behavior.ocelotattack": {
        "priority": 4,
        "walk_speed_multiplier": 0.8,
        "sprint_speed_multiplier": 1.33,
        "sneak_speed_multiplier": 0.6
      }
    },


    "events": {
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 9,
            "remove": {},
            "add": { "component_groups": ["minecraft:adult", "minecraft:wild", "cg:interact"]}
          },
          {
            "weight": 3,
            "remove": {},
            "add": {"component_groups": ["minecraft:baby", "minecraft:wild", "cg:interact"]}
          }
        ]
      },
      "minecraft:entity_born": {
        "add": {  "component_groups": [ "minecraft:baby", "minecraft:wild", "cg:interact" ]  }
      },
      "minecraft:ageable_grow_up": {
          "remove": {   "component_groups": ["minecraft:baby"]  },
          "add": {  "component_groups": [   "minecraft:adult"    ]}
      },


      "minecraft:on_trust": {
        "sequence": [
          {
            "remove": {
              "component_groups": [
                "minecraft:wild"
              ]
            }
          },
          {
            "add": {
              "component_groups": [
                "minecraft:trusting"
              ]
            }
          }
        ]
      }
    }
  }
}
