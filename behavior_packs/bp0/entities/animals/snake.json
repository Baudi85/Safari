{
    "format_version": "1.8.0",
    "minecraft:entity": {

      "description": {
        "identifier": "animal:snake",
        "is_spawnable": true,
        "is_summonable": true,
        "is_experimental": false
      },


      "component_groups": {
        
        "cg:interact" : {
          "minecraft:interact": [
            {
             "on_interact": {  "filters": {"all_of": [  {"test": "has_equipment","domain": "hand","subject": "other","value": "gameplay:cotton_bud"} ]}},
             "use_item": true,
             "swing": true,
             "interact_text" : "action.interact.take_dna",
             "spawn_items": {"table": "loot_tables/dna/21.json"},
             "play_sounds": "random.pop"
            }
          ]
        }
      },
  
  
      "components": {

        "minecraft:type_family": {     "family": [ "mob" , "snake"]     },
        "minecraft:physics": {},
        "minecraft:health": {    "value": 10,   "max": 10   },
        "minecraft:movement.basic": {},
        "minecraft:loot": {
          "table": "loot_tables/entities/snake.json"
        },
        "minecraft:jump.static": {},
        "minecraft:can_climb": {},
        "minecraft:collision_box": {
            "width": 1,
            "height": 0.6
        },
        "minecraft:movement": {  "value": 0.15 },
        "minecraft:navigation.walk": {   "can_path_over_water": true,   "avoid_water": true,   "avoid_damage_blocks": true   },
        "minecraft:behavior.panic": {
          "priority": 3,
          "speed_multiplier": 1.25
        },
        "minecraft:behavior.random_stroll": {
          "priority": 7,
          "speed_multiplier": 1.0
        },
        "minecraft:behavior.look_at_player": {
          "priority": 8,
          "look_distance": 6.0,
          "probability": 0.02
        },
        "minecraft:behavior.random_look_around": {
          "priority": 9
        },
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:scale": {     "value": 1   },
        "minecraft:behavior.mount_pathing": {
          "priority": 1,
          "speed_multiplier": 1.25,
          "target_dist": 0.0,
          "track_target": true
        },
        "minecraft:behavior.float": {
          "priority": 2
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 2,
          "entity_types": [
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "victim"
              },
              "max_dist": 48
            },
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "player"
              },
              "max_dist": 8
            }
          ],
          "must_see": true
        },
        "minecraft:behavior.melee_attack": {
          "priority": 5,
          "target_dist": 1.2,
          "track_target": true,
          "reach_multiplier": 1.0
        },
        "minecraft:attack": {
          "damage": 8
        },
        "minecraft:behavior.breed": {
          "priority": 4,
          "speed_multiplier": 1.0
        },
        "minecraft:breedable": {
          "requireTame": false,
          "breedsWith": {
            "mateType": "animal:snake",
            "babyType": "animal:snake"
          },
          "breedItems": [
            "steak"
          ]
        }

      },
  
  
      "events": {
        "minecraft:entity_spawned": {
          "add": { "component_groups": [ "cg:interact"]}
        }
      }
    }
  }
  