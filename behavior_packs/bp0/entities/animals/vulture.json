{
  "format_version": "1.8.0",
  "minecraft:entity": {

    "description": {
      "identifier": "animal:vulture",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },

    "component_groups": {
      
      "cg:interact" : {
        "minecraft:interact": [
          {
           "on_interact": {  "filters": {"all_of": [  {"test": "has_equipment","domain": "hand","subject": "other","value": "gameplay:cotton_bud"} ]}},
           "use_item": true,
           "swing": true,
           "interact_text" : "action.interact.take_dna",
           "spawn_items": {"table": "loot_tables/dna/22.json"},
           "play_sounds": "random.pop"
          }
        ]
      },
      "baby": {
        "minecraft:is_baby": {
        },
        "minecraft:scale": {
          "value": 0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feedItems": "beef",
          "grow_up": {
            "event": "grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 6,
          "speed_multiplier": 1.1
        }
      },

      "adult": {
        "minecraft:scale": {
          "value": 1.0
        },
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:breedable": {
          "requireTame": false,
          "breedsWith": {
            "mateType": "animal:vulture",
            "babyType": "animal:vulture"
          },
          "breedItems": "beef"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/vulture.json"
        }
      },
      
      "flying": {
        "minecraft:can_fly": {
        },
        "minecraft:movement.glide": {
          "start_speed": 0.1,
          "speed_when_turning": 0.2
        },
        "minecraft:behavior.swoop_attack": {
          "priority": 2,
          "delay_range": [ 10.0, 20.0 ]
        },
        "minecraft:physics": {
          "has_gravity": false
        },
        "minecraft:behavior.circle_around_anchor": {
          "priority": 3,
          "radius_range": [ 5.0, 15.0 ],
          "radius_change_chance": 250,
          "height_above_target_range": [ 10.0, 30.0 ],
          "height_offset_range": [ -4.0, 5.0 ],
          "height_change_chance": 350,
          "goal_radius": 1.0
        }
      },

      "walking": {
        "minecraft:navigation.walk": {
          "can_path_over_water": true,
          "avoid_water": true,
          "avoid_damage_blocks": true
        },
        "minecraft:movement.basic": {
        },
        "minecraft:jump.static": {
        },
        "minecraft:can_climb": {
        },
        "minecraft:physics": {
          "has_gravity": true
        }
      }
    },

    "components": {
      "minecraft:timer": {
        "time": [
          10,
          30
        ],
        "looping": true,
        "time_down_event": {
          "event": "switcher",
          "target": "self"
        }
      },
      "minecraft:breathable": {
        "totalSupply": 15,
        "suffocateTime": 0
      },
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 1
      },
      "minecraft:nameable": {
      },
      "minecraft:health": {
        "value": 6,
        "max": 6
      },
      "minecraft:movement": {
        "value": 0.2
      },
      "minecraft:fall_damage": {
        "value": 0.0
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 0,
        "speed_multiplier": 1.25
      },
      "minecraft:behavior.look_at_player": {
        "priority": 1,
        "look_distance": 8.0
      },
      "minecraft:behavior.random_stroll": {
        "priority": 7,
        "speed_multiplier": 0.8
      },
      "minecraft:behavior.random_look_around": {
        "priority": 9
      }
    },

    "events": {
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 5,
                "remove": {
                },
                "add": {
                  "component_groups": [
                    "adult",
                    "walking", "cg:interact"
                  ]
                }
              },
              {
                "weight": 5,
                "remove": {
                },
                "add": {
                  "component_groups": [
                    "baby",
                    "walking", "cg:interact"
                  ]
                }
              }
            ]
          }
        ]
      },
      
      "minecraft:entity_born": {
        "remove": {
        },
        "add": {
          "component_groups": [
            "baby",
            "walking", "cg:interact"
          ]
        }
      },

      "grow_up": {
        "remove": {
          "component_groups": [
            "baby"
          ]
        },
        "add": {
          "component_groups": [
            "adult"
          ]
        }
      },

      "switcher": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 5,
                "remove": {
                  "component_groups": [
                    "flying"
                  ]
                },
                "add": {
                  "component_groups": [
                    "walking"
                  ]
                }
              },
              {
                "weight": 5,
                "remove": {
                  "component_groups": [
                    "walking"
                  ]
                },
                "add": {
                  "component_groups": [
                    "flying"
                  ]
                }
              }
            ]
          }
        ]
      }
    }
  }
}