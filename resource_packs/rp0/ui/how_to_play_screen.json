{
  "namespace": "how_to_play",

  "how_to_play_screen@how_to_play_common.screen_base": {
    "$section_content_panels": "how_to_play.section_content_panels",
    "$selector_stack_panel": "how_to_play.selector_stack_panel",
    "$header_safezone_control": "common_store.store_top_bar_filler",
    "$header_bar_control": "common_store.store_top_bar",
    "$fill_alpha": 1.0,
    "$is_full_screen_layout": true
    
  },

  "selector_stack_panel": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "orientation": "vertical",
    "$default_selector_toggle_index": 0,
    "controls": [
      {
        "how_to_play_selector_pane": {
          "type": "stack_panel",
          "controls": [
            {
              "padding_0": {
                "type": "panel",
                "size": [ 0, 6 ]
              }
            },


            { "about_us_buton@how_to_play.about_us_buton":      { "$toggle_group_forced_index": 3 } },
            
            { "map_category@how_to_play_common.topic_category": { "$category": "map" } },
            { "map_button@how_to_play.map_button":              { "$toggle_group_forced_index": 4 } },
            { "mines_button@how_to_play.base_button":           { "$toggle_group_forced_index": 5 } },
            { "mines_button@how_to_play.mines_button":          { "$toggle_group_forced_index": 6 } },
            { "gameplay_category@how_to_play_common.topic_category": { "$category": "gameplay" }},

            { "animals_button@how_to_play.animals_button":      { "$toggle_group_forced_index": 7 } },
            { "dna_button@how_to_play.dna_button":              { "$toggle_group_forced_index": 8 } },
            { "energy_button@how_to_play.energy_button":        { "$toggle_group_forced_index": 9 } },
            { "ores_button@how_to_play.ores_button":  { "$toggle_group_forced_index": 10 } },
            { "craftings_button@how_to_play.craftings_button":  { "$toggle_group_forced_index": 11 } },
            { "gettingstarted_button@how_to_play.gettingstarted_button":  { "$toggle_group_forced_index": 12 } },
            {
              "padding_1@how_to_play_common.padding": {
                "size": [ 0, 10 ]
              }
            }
          ]
        }
      }
    ]
  },

  "section_content_panels": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100% - 5px", "100%c + 5px" ],
    "offset": [ 2, 0 ],
    "controls": [
      {
        "general_tips_sections": {
          "type": "stack_panel",
          "controls": [
            {
              "section_contents_header@how_to_play_common.section_contents_header": {}
            },
            { "about_us_section@how_to_play.about_us_section": {} },
            { "map_section@how_to_play.map_section": {} },
            { "base_section@how_to_play.base_section": {} },
            { "mines_section@how_to_play.mines_section": {} },
            { "animals_section@how_to_play.animals_section": {} },
            { "dna_section@how_to_play.dna_section": {} },
            { "energy_section@how_to_play.energy_section": {} },
            { "ores_section@how_to_play.ores_section": {} },
            { "craftings_section@how_to_play.craftings_section": {} },
            { "gettingstarted_section@how_to_play.gettingstarted_section": {} },
            {
              "section_contents_footer@how_to_play_common.section_contents_footer": {}
            }
          ]
        }
      }
    ]
  },

